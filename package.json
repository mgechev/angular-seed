    "run-sequence": "^1.1.0",
    "semver": "^5.0.3",
    "serve-static": "^1.9.2",
    "slash": "~1.0.0",
    "stream-series": "^0.1.1",
    "tiny-lr": "^0.2.1",
    "traceur": "^0.0.91",
    "ts-node": "^0.5.4",
    "typedoc": "^0.3.12",
    "typescript": "~1.7.3",
    "typings": "^0.6.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^3.32.0"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.1",
